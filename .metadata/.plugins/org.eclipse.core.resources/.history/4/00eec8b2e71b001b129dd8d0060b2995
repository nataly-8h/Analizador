import java.util.ArrayList;
import java.util.Arrays;
import java.util.Hashtable;

public class CYK {
	public Hashtable<String, ArrayList<String>> CYKgrammar;
	
	
	public CYK(Hashtable<String, ArrayList<String>> cYKgrammar) {
		CYKgrammar = cYKgrammar;
		System.out.println("CYK " + this.CYKgrammar.toString());
	}


	public static void main(String[] args) {
		Analizador a = new Analizador("S→AB|SS|AC|BD|BA A→a B→b C→SB D→SA");
		System.out.println(a.grammar.toString());
		CYK b = new CYK(a.grammar);
		//Hashtable<String, ArrayList<String>> prueba = A;
		//prueba.put("S", new ArrayList<String>(Arrays.asList("AB|SS|AC|BD|BA")));
		
	}

}
